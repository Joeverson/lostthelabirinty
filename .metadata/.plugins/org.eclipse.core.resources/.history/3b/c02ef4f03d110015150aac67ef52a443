package controllers;

import java.awt.List;

import models.ObjectLabirinty;


public class GenerateLabirinty {
	public GenerateLabirinty() {}
	
	public static void generate(int i){

		private static ObjectLabirinty[][] labirinties = new ObjectLabirinty()[i][i];
		
		int n = 0;
		
		for(int k = 0; k < i; k++){
			for(int j = 0; j < i; j++){
				labirinties[k][j] = new ObjectLabirinty(k, j, n);
				System.out.println(labirinties[k][j].n());
				n++;
			}
		}
	}

}
